<table>
<tr>
  <th></th>
  <th>Hours</th>
  <th>Url</th>
</tr>

<% projects.each do |project| %>
	<tr valign='top' class='row<%= cycle(1, 2) %>'>
    <td><%= link_to(project.name, project) %></td>
    <td><%= project.hours %></td>
    <td><%= link_to_if(!project.url.blank?, 'Project URL', project.url) %></td>
  </tr>
<% end %>
</table>

<br />
